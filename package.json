        "@fastify/websocket": "^8.2.0",
        "@google/genai": "^1.29.1",
        "@modelcontextprotocol/sdk": "^1.20.1",
        "@perplexity-ai/perplexity_ai": "^0.16.0",
        "@supabase/supabase-js": "^2.38.0",
        "axios": "^1.6.2",
        "bullmq": "^5.9.0",
        "cheerio": "^1.1.2",
        "dotenv": "^16.4.5",
        "dotenv-expand": "^12.0.3",
        "fastify": "^4.24.3",
        "ioredis": "^5.3.2",
        "langchain": "^0.3.36",
        "mongoose": "^9.0.0",
        "pino": "^10.1.0",
